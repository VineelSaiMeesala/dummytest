        "headers": {
          "Authorization": "Bearer {{token}}"
        },
        "message": "{\n  \"action\": \"SEND_NOTIFICATION\",\n  \"channel\": \"{{wsChannel}}\",\n  \"payload\": {{wsPayload}}\n}"
      },
      "request": {
        "method": "GET",
        "url": {
