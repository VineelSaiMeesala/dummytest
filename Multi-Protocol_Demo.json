    {
      "name": "3) gRPC Get Plan Details",
      "requestMode": "grpc",
      "data": {
        "isran": false
      },
      "event": [],
      "flow": {
        "extractVariables": {
          "wsPayload": "websocket_payload"
        }
      },
      "grpc": {
        "url": "{{GrpcBaseUrl}}",
        "service": "plans.PlanService",
        "method": "GetPlanDetails",
        "message": "{ \"id\": \"{{planId}}\", \"topic\": \"{{grpcTopic}}\" }",
        "metadata": {
          "x-auth-token": "{{token}}"
        },
        "streamType": "unary",
        "secure": true,
        "protoFile": {
          "name": "plan.proto",
          "content": "syntax = \"proto3\";\npackage plans;\nservice PlanService { rpc GetPlanDetails(PlanRequest) returns (PlanResponse); }\nmessage PlanRequest { string id = 1; string topic = 2; }\nmessage PlanResponse { string id = 1; string name = 2; string websocket_payload = 3; }"
        }
      },
      "request": {
        "method": "GET",
        "url": {
          "raw": "{{GrpcBaseUrl}}",
          "host": [
            "{{GrpcBaseUrl}}"
          ],
          "path": []
        }
      },
      "id": "cfe4b392-5c16-483e-80fb-951051398915"
    },
