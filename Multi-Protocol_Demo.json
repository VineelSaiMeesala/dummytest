    {
      "name": "2) GraphQL Get User Profile",
      "requestMode": "graphql",
      "data": {
        "isran": false
      },
      "event": [],
      "flow": {
        "extractVariables": {
          "planId": "data.user.plan.id",
          "grpcTopic": "data.user.plan.grpcTopic",
          "wsChannel": "data.user.wsChannel"
        }
      },
      "graphql": {
        "url": "{{GraphQLBaseUrl}}/graphql",
        "query": "query GetUser($id: ID!) { user(id: $id) { id email plan { id name grpcTopic } wsChannel } }",
        "variables": "{ \"id\": \"{{userId}}\" }",
        "operationName": "GetUser",
        "headers": {
          "Content-Type": "application/json",
          "Authorization": "Bearer {{token}}"
        }
      },
      "request": {
        "method": "POST",
        "header": [
          {
            "key": "Content-Type",
            "value": "application/json"
          },
          {
            "key": "Authorization",
            "value": "Bearer {{token}}"
          }
        ],
        "url": {
          "raw": "{{GraphQLBaseUrl}}/graphql",
          "host": [
            "{{GraphQLBaseUrl}}"
          ],
          "path": [
            "graphql"
          ]
        },
        "body": {
          "mode": "graphql",
          "graphql": {
            "query": "query GetUser($id: ID!) { user(id: $id) { id email plan { id name grpcTopic } wsChannel } }",
            "variables": "{ \"id\": \"{{userId}}\" }"
          }
        }
      },
      "id": "0e68df34-b839-4d39-a148-9c40456378b4"
